{
  "name": "npm-validation",
  "version": "1.0.0",
  "description": "Plugin to validate dependency's, concerning their license and vulnerabities",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint",
    "lint": "eslint . --fix",
    "postlint": "npm run clean && npm run readTest ",
    "readTest": "node lib/index.js read && npm run writeTest",
    "writeTest": "node lib/index.js write",
    "clean": "cd lib/ && del *.txt",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  },
  "dependencies": {
    "nsp": "3.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RuiDTLima/OSDA-Open-Source-Dependencies-Analysis.git"
  },
  "license": "BSD-3-Clause"
}
