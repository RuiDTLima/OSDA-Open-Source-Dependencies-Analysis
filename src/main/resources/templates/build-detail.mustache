{{> header}}

<div class="build-detail">

    <h3>
        Project Name: {{project_name}}
    </h3>
    <br>
    Timestamp: {{timestamp}}
    <br>
    Tag: {{tag}}
    <br>

    <label for="dependency-list">
        <h2>
            Dependencies
        </h2>
    </label>
    <ul class="list-group" id="dependency-list">
        {{#dependencies}}
            <li class="list-group-item">
                Project Name: {{pk.id}}
                <br>
                Main Version: {{pk.mainVersion}}
                <br>
                {{#license}}
                    License Name: {{pk.license.spdxId}}
                    <br>
                    License Source: {{source}}
                {{/license}}
                <br>
                <a>Detail</a>
            </li>
        {{/dependencies}}
    </ul>

</div>

{{> footer}}