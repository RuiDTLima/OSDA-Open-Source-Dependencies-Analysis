
{{#dependencies}}
<script>
filterVulnerableDependencies({{vulnerabilitiesCount}},'<li class="%s">' +
'               <a href="/report/project/{{project_name}}/build/{{build_id}}/dependency/{{pk.id}}/{{pk.mainVersion}}/detail" class="list-group-item list-group-item-action">' +
'                Project Name: {{pk.id}}\n' +
'                <br>\n' +
'                Main Version: {{pk.mainVersion}}\n' +
'                <br>\n' +
'                {{#license}}\n'+
'                    License Name: {{pk.license.spdxId}} ' +
'                {{/license}}\n' +
'                 <br> ' +
'                 {{vulnerabilitiesCount}} vulnerabilities found'+
'                <br>\n' +
'                </a>\n' +
'            </li>')
</script>
{{/dependencies}}


<div class="btn-group btn-group-toggle text-center" data-toggle="buttons">
<label class="btn btn-outline-dark text-center active">
<input type="radio" name="options" id="option1" autocomplete="off" checked> Detail
</label>
<label class="btn btn-outline-dark text-center">
<input type="radio" name="options" id="option2" autocomplete="off" onclick="filterInformation('option2')"> Licenses
</label>
<label class="btn btn-outline-dark text-center">
<input type="radio" name="options" id="option3" autocomplete="off"> Vulnerabilities
</label>
</div>